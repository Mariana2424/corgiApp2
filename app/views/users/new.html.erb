<%= form_for(@user, :html => {class: "form-control", role: "form"}) do |t| %>
  <%= t.label :username %>
  <%= t.text_field :username %>
  <%= t.label :email %>
  <%= t.email_field :email %>
  <%= t.label :photo %>
  <%= t.text_field :photo %>
  <%= t.label :password %>
  <%= t.password_field :password %>
  <%= t.submit "Sign up" %>
<% end %>